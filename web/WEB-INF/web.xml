<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>addcompanyservlet</servlet-name>
        <servlet-class>myPackage.addcompanyservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addemployeeservlet</servlet-name>
        <servlet-class>myPackage.addemployeeservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>companycheckservlet</servlet-name>
        <servlet-class>myPackage.companycheckservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addCompanyservlet1</servlet-name>
        <servlet-class>myPackage.addCompanyservlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>companycheckservlet1</servlet-name>
        <servlet-class>myPackage.companycheckservlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>feedbackservlet</servlet-name>
        <servlet-class>myPackage.feedbackservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addcompanyservlet</servlet-name>
        <url-pattern>/addcompanyservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addemployeeservlet</servlet-name>
        <url-pattern>/addemployeeservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>companycheckservlet</servlet-name>
        <url-pattern>/companycheckservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addCompanyservlet1</servlet-name>
        <url-pattern>/addCompanyservlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>companycheckservlet1</servlet-name>
        <url-pattern>/companycheckservlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feedbackservlet</servlet-name>
        <url-pattern>/feedbackservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
